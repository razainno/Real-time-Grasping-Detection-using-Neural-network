  <arg name="transmission_hw_interface" default="hardware_interface/PositionJointInterface" />

  <param unless="$(arg limited)" name="robot_description" command="$(find xacro)/xacro --inorder '$(find gripper_ur5)/urdf/ur5_gripper.urdf.xacro' transmission_hw_interface:=$(arg transmission_hw_interface)" />
  <param if="$(arg limited)" name="robot_description" command="$(find xacro)/xacro --inorder '$(find gripper_ur5)/urdf/ur5_gripper.urdf.xacro' transmission_hw_interface:=$(arg transmission_hw_interface)" />
</launch>
